<Nm>{{ organization.name|escape }}</Nm>
{% if organization.identification|length > 0 %}
<Id>
	<OrgId>
	{% if organization.identification.bicorbei is not empty %}
		<BICOrBEI>{{ organization.identification.bicorbei }}</BICOrBEI>
	{% else %}
		<Othr>
			<Id>{{ organization.identification['kbo-bce'] }}</Id>
			<Issr>KBO-BCE</Issr>
		</Othr>
	{% endif %}
	</OrgId>
</Id>
{% endif %}
